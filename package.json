{
  "name": "template-fullstack",
  "version": "2.0.0",
  "description": "project template for the React-Express-MySQL cursus in Wild Code School",
  "homepage": "https://github.com/wildcodeschool/js-template-fullstack#readme",
  "bugs": {
    "url": "https://github.com/wildcodeschool/js-template-fullstack/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wildcodeschool/js-template-fullstack.git"
  },
  "config": {
    "cli": "npm"
  },
  "scripts": {
    "postinstall": "cross-env-shell \"husky install && cd ./front-web && npm install && cd ../backend && npm install && cd ../rn && npm install\"",
    "migrate": "cd ./backend && node migrate.js",
    "dev": "cross-env-shell 'concurrently -c green,yellow -t \"HH:mm:ss\" -p \"{name} {time}\" \"npm run dev-*\"'",
    "dev-front": "cross-env-shell \"cd ./front-web && npm run dev\"",
    "dev-back": "cross-env-shell \"cd ./backend && npm run dev\"",
    "lint": "cross-env-shell \"cd ./front-web && npm run lint && cd ../backend && npm run lint\"",
    "fix:front-web": "cd front-web && npm run fix",
    "fix:backend": "cd backend && npm run fix",
    "fix:rn": "cd rn && npm run fix",
    "fix": "npm run fix:front-web && npm run fix:backend && npm run fix:rn"
  },
  "keywords": [],
  "author": "Wild Code School",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3"
  }
}
